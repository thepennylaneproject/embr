{
  "name": "embr-api",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "nest start",
    "start:dev": "nest start --watch",
    "build": "nest build",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "db:migrate:dev": "prisma migrate dev --schema ../../prisma/schema.prisma",
    "db:migrate:deploy": "prisma migrate deploy --schema ../../prisma/schema.prisma",
    "db:migrate:reset": "prisma migrate reset --schema ../../prisma/schema.prisma",
    "db:migrate:status": "prisma migrate status --schema ../../prisma/schema.prisma",
    "db:generate": "prisma generate --schema ../../prisma/schema.prisma",
    "db:push": "prisma db push --schema ../../prisma/schema.prisma",
    "db:studio": "prisma studio --schema ../../prisma/schema.prisma",
    "db:validate": "prisma validate --schema ../../prisma/schema.prisma",
    "db:format": "prisma format --schema ../../prisma/schema.prisma"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-socket.io": "^10.3.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/event-emitter": "^2.0.0",
    "@nestjs/websockets": "^10.3.0",
    "@prisma/client": "^5.8.0",
    "@aws-sdk/client-s3": "^3.656.0",
    "@aws-sdk/s3-request-presigner": "^3.656.0",
    "@mux/mux-node": "^8.0.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "sharp": "^0.33.5",
    "stripe": "^14.25.0",
    "uuid": "^9.0.1",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.2",
    "@nestjs/testing": "^10.3.0",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^4.0.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.21",
    "@types/multer": "^1.4.12",
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
